<h1><%= @list.name %></h1>

<%= link_to 'Create bookmark', new_list_bookmark_path(@list) %>

<ul>
  <% @list.bookmarks.each do |bookmark| %>
  <li><%= bookmark.comment %> <%= bookmark.movie.title %> <%= bookmark.movie.overview %> <%= bookmark.movie.rating %> <%= link_to "Delete", bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}  %></li>
  <% end %>
</ul>
